n    async getSujetById(context, id) {\n\n      try{\n\n        const { data: sujet } = await axios.get( ApiUrl +\"/getById/\" + id);\n        context.commit(\"SET_SUJET\", sujet);\n\n      }catch (exception) {\n\n          swal(\"Error\", \"Quelque chose s'est mal passé \", \"error\");\n\n      }\n\n    },\n  },\n\n}\n"]}]}