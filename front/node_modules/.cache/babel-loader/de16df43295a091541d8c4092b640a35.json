AA,KADA;AAEA,IAAA,QAAA,EAAA,kBAAA,KAAA;AAAA,aAAA,KAAA,CAAA,OAAA,CAAA,QAAA;AAAA;AAFA,GAAA,CADA;AAtBA,CAAA","sourcesContent":["<template>\n  <v-row class=\"mt-12\">\n    <v-list width=\"100%\">\n      <v-list-group\n        v-for=\"(categorie, key) in categories\"\n        :key=\"key\"\n        no-action\n      >\n        <template v-slot:activator>\n          <v-list-item-content>\n\n            <h1 \n              style=\"font-size: 20px\" \n              v-text=\"categorie.name\"\n            ></h1>\n\n          </v-list-item-content>\n        </template>\n\n        <v-list-item\n          v-for=\"(section, key) in filterSection(\n            categorie._id\n          )\"\n          :key=\"key\"\n          :to=\"{ path: '/listSujet/' + section._id }\"\n        >\n          <v-list-item-icon class=\"mt-6 grey--text text--darken-2\">\n            <v-icon\n              v-text=\"section.icon\"\n              style=\"font-size: 30px\"\n            ></v-icon>\n          </v-list-item-icon>\n\n          <v-list-item-content>\n            <div class=\"d-flex flex-row justify-start align-start\">\n              <div\n                class=\"subtitle-1 mt-1 grey--text text--darken-4\"\n                v-text=\"section.name\"\n              ></div>\n              <v-chip x-small class=\"ma-2 grey\"> nouveaux </v-chip>\n            </div>\n            <v-list-item-title\n              class=\"subtitle-2 grey--text\"\n              v-text=\"section.description\"\n            ></v-list-item-title>\n          </v-list-item-content>\n\n          <div class=\"d-flex flex-row justify-center align-center\">\n            <v-list-item-avatar class=\"mr-5\">\n              <v-img :src=\"section.avatar\"></v-img>\n            </v-list-item-avatar>\n            <v-list-item-title>\n              <div class=\"subtitle-1 black--text\">titre du sujet</div>\n              <div class=\"subtitle-2 grey--text\">26 juin - by pseudo</div>\n            </v-list-item-title>\n          </div>\n\n        </v-list-item>\n      </v-list-group>\n    </v-list>\n  </v-row>\n</template>\n\n<script>\nimport { mapActions, mapState } from \"vuex\";\n\nexport default {\n  data() {\n    return {};\n  },\n  created() {\n    this.getCategorie();\n    this.getSections();\n  },\n  methods: {\n    ...mapActions([\"getCategorie\", \"getSections\"]),\n\n    filterSection(bId) {\n      var arrayCat = [];\n      this.sections.forEach((item) => {\n        if (bId === item.categorie_id) {\n          arrayCat.push(item);\n        }\n      });\n\n      return arrayCat;\n    },\n  },\n  computed: {\n    ...mapState({\n      categories: state => state.categorie.categories, \n      sections: state => state.section.sections\n      }),\n  },\n};\n</script>\n"],"sourceRoot":"src/components/pages/forum"}]}